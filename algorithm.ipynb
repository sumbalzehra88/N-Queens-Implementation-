    <script type="module">
      import * as THREE from "https://cdn.jsdelivr.net/npm/three@0.164.1/build/three.module.js";

      // ---------- three.js scene (unchanged visuals) ----------
      const scene = new THREE.Scene();
      scene.background = new THREE.Color(0x0a0a15);
